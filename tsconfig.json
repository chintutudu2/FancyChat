{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/Assets/*"],
      "@components/*": ["src/Components/*"],
      "@config/*": ["src/Config/*"],
      "@constants/*": ["src/Constants/*"],
      "@helpers/*": ["src/Helpers/*"],
      "@navigators/*": ["src/Navigators/*"],
      "@screens/*": ["src/Screens/*"],
      "@utils/*": ["src/Utils/*"]
    }
  }
}
